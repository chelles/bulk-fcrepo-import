<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="edu.yale.library.ladybird.entity.AccessconditionType" table="accesscondition_type" catalog="pamoja">
        <id name="accessconditionTypeId" type="java.lang.Integer">
            <column name="accesscondition_type_id" />
            <generator class="identity" />
        </id>
        <property name="date" type="timestamp">
            <column name="date" length="19" not-null="true" />
        </property>
        <property name="userId" type="int">
            <column name="user_id" not-null="true" />
        </property>
        <property name="label" type="string">
            <column name="label" length="45" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>
