<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="edu.yale.library.ladybird.entity.ProjectTemplateStrings" table="project_template_strings" catalog="pamoja">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="templateId" type="java.lang.Integer">
            <column name="template_id" />
        </property>
        <property name="fdid" type="java.lang.Integer">
            <column name="fdid" />
        </property>
        <property name="value" type="string">
            <column name="value" length="1000" />
        </property>
    </class>
</hibernate-mapping>
