<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="edu.yale.library.beans.FieldMarcMapping" table="field_marc_mapping" catalog="pamoja">
        <id name="id" type="int">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="date" type="timestamp">
            <column name="date" length="19" />
        </property>
        <property name="k1" type="string">
            <column name="k1" length="3" />
        </property>
        <property name="k2" type="string">
            <column name="k2" length="3" />
        </property>
        <property name="concat" type="string">
            <column name="concat" length="1" />
        </property>
        <property name="delim" type="string">
            <column name="delim" length="10" />
        </property>
        <property name="fdid" type="java.lang.Integer">
            <column name="fdid" />
        </property>
    </class>
</hibernate-mapping>
