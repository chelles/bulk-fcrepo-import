<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="edu.yale.library.beans.HydraPublishPath" table="hydra_publish_path" catalog="pamoja">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="date" type="timestamp">
            <column name="date" length="19" not-null="true" />
        </property>
        <property name="hydraPublishId" type="int">
            <column name="hydra_publish_id" not-null="true" />
        </property>
        <property name="type" type="string">
            <column name="type" length="45" />
        </property>
        <property name="pathhttp" type="string">
            <column name="pathhttp" length="500" />
        </property>
        <property name="pathunc" type="string">
            <column name="pathunc" length="500" />
        </property>
        <property name="md5" type="string">
            <column name="md5" length="32" />
        </property>
        <property name="sha256" type="string">
            <column name="sha256" length="64" />
        </property>
        <property name="controlgroup" type="string">
            <column name="controlgroup" length="10" />
        </property>
        <property name="mimetype" type="string">
            <column name="mimetype" length="50" />
        </property>
        <property name="dsid" type="string">
            <column name="dsid" length="150" />
        </property>
        <property name="ingestmethod" type="string">
            <column name="ingestmethod" length="50" />
        </property>
        <property name="altids" type="string">
            <column name="altids" length="50" />
        </property>
        <property name="dslabel" type="string">
            <column name="dslabel" length="50" />
        </property>
        <property name="versionable" type="java.lang.Integer">
            <column name="versionable" />
        </property>
        <property name="dsstate" type="string">
            <column name="dsstate" length="50" />
        </property>
        <property name="logmessage" type="string">
            <column name="logmessage" length="500" />
        </property>
        <property name="oidpointer" type="java.lang.Integer">
            <column name="oidpointer" />
        </property>
        <property name="fileSize" type="java.lang.Integer">
            <column name="file_size" />
        </property>
        <property name="fileExtension" type="string">
            <column name="file_extension" length="15" />
        </property>
    </class>
</hibernate-mapping>
