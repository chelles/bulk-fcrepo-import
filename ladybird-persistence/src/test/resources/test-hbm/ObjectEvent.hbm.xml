<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="edu.yale.library.entity.model.ObjectEvent" table="object_event" catalog="pamoja">
        <id name="eventId" type="java.lang.Integer">
            <column name="event_id" />
            <generator class="identity" />
        </id>
        <property name="eventTypeId" type="int">
            <column name="event_type_id" not-null="true" />
        </property>
        <property name="userId" type="int">
            <column name="user_id" not-null="true" />
        </property>
        <property name="date" type="timestamp">
            <column name="date" length="19" not-null="true" />
        </property>
        <property name="oid" type="int">
            <column name="oid" not-null="true" />
        </property>
        <property name="event" type="string">
            <column name="event" length="500" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
