<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="edu.yale.library.ladybird.entity.Permissions" table="permissions" catalog="pamoja">
        <id name="permissionsId" type="java.lang.Integer">
            <column name="permissions_id" />
            <generator class="identity" />
        </id>
        <property name="permissionsName" type="string">
            <column name="permissions_name" length="45" />
        </property>
        <property name="permissionsDesc" type="string">
            <column name="permissions_desc" length="45" />
        </property>
    </class>
</hibernate-mapping>
