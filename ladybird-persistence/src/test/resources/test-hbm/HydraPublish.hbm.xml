<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="edu.yale.library.ladybird.entity.HydraPublish" table="hydra_publish" catalog="pamoja">
        <id name="hydraPublishId" type="java.lang.Integer">
            <column name="hydra_publish_id" />
            <generator class="identity" />
        </id>
        <property name="date" type="timestamp">
            <column name="date" length="19" />
        </property>
        <property name="hydraContentmodelId" type="int">
            <column name="hydra_contentmodel_id" />
        </property>
        <property name="collectionId" type="int">
            <column name="collection_id" />
        </property>
        <property name="projectId" type="int">
            <column name="project_id" />
        </property>
        <property name="oid" type="int">
            <column name="oid" />
        </property>
        <!--
        <property name="oid_1" type="int">
            <column name="_oid" />
        </property>
        -->
        <property name="zindex" type="int">
            <column name="zindex" />
        </property>

        <property name="hydraid" type="string">
            <column name="hydraid" length="45" />
        </property>
        <property name="action" type="string">
            <column name="action" length="10" />
        </property>
        <property name="dateready" type="timestamp">
            <column name="dateready" length="19" />
        </property>
        <property name="datehydrastart" type="timestamp">
            <column name="datehydrastart" length="19" />
        </property>
        <property name="datehydraend" type="timestamp">
            <column name="datehydraend" length="19" />
        </property>
        <property name="dateaudit" type="timestamp">
            <column name="dateaudit" length="19" />
        </property>
        <property name="attempts" type="int">
            <column name="attempts" />
        </property>
        <property name="priority" type="int">
            <column name="priority" />
        </property>
        <property name="server" type="string">
            <column name="server" length="45" />
        </property>
    </class>
</hibernate-mapping>
