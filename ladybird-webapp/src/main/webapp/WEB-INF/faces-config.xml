<?xml version="1.0" encoding="utf-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
              version="2.0">

    <navigation-rule>
        <from-view-id>/pages/secure/user_manager_form.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>ok</from-outcome>
            <to-view-id>/pages/secure/user_manager.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>failed</from-outcome>
            <to-view-id>/pages/form_submission_failed.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pages/secure/import/import_source_form.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>ok</from-outcome>
            <to-view-id>/pages/secure/import/import_source.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>failed</from-outcome>
            <to-view-id>/pages/form_submission_failed.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pages/secure/projects_form.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>ok</from-outcome>
            <to-view-id>/pages/secure/projects.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pages/secure/import/import_engine_file.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>ok</from-outcome>
            <to-view-id>/pages/secure/import/import_engine.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>failed</from-outcome>
            <to-view-id>/pages/form_submission_failed.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pages/secure/import/import_engine_dir.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>ok</from-outcome>
            <to-view-id>/pages/secure/import/import_engine.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>failed</from-outcome>
            <to-view-id>/pages/form_submission_failed.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

</faces-config>
