<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="edu.yale.library.ladybird.entity.ObjectVersion" table="object_version" catalog="pamoja">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="oid" type="java.lang.Integer">
            <column name="oid" />
        </property>
        <property name="versionId" type="java.lang.Integer">
            <column name="version_id" />
        </property>
        <property name="creationDate" type="timestamp">
            <column name="creation_date" length="19" />
        </property>
        <property name="userId" type="java.lang.Integer">
            <column name="user_id" />
        </property>
        <property name="notes" type="string">
            <column name="notes" length="45" />
        </property>
    </class>
</hibernate-mapping>
