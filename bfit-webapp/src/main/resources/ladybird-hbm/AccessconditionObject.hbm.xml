<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="edu.yale.library.ladybird.entity.AccessconditionObject" table="accesscondition_object" catalog="pamoja">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="date" type="timestamp">
            <column name="date" length="19" />
        </property>
        <property name="oid" type="int">
            <column name="oid" />
        </property>
        <property name="accessconditionTypeId" type="int">
            <column name="accesscondition_type_id" />
        </property>
        <property name="accessconditionTargetId" type="int">
            <column name="accesscondition_target_id" />
        </property>
        <property name="value" type="string">
            <column name="value" length="250" />
        </property>
    </class>
</hibernate-mapping>
